{
  "name": "dersonsena/yii2-user-module",
  "description": "Implementação padrão de um Módulo de Usuários para meus projetos pessoais no Yii2",
  "keywords": [
    "yii2",
    "user-module",
    "yii2 user module"
  ],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Kilderson Sena",
      "email": "dersonsena@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.4",
    "yiisoft/yii2-bootstrap": "*",
    "kartik-v/yii2-widget-activeform": "^1.4",
    "kartik-v/yii2-password": "^1.5",
    "dersonsena/yii2-common-classes" : "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "dersonsena\\userModule\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
